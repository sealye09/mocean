{
  "name": "@mocean/mastra",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dotenvx": "dotenvx",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "serve": "dotenvx run -- mastra dev",
    "build": "mastra build",
    "bundle": "rollup -c",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "import-agents": "dotenvx run -- tsx scripts/import-agents.ts",
    "import-providers-models": "tsx scripts/import-providers-models.ts",
    "import-assistants": "tsx scripts/import-assistants.ts",
    "check-types": "tsc --noEmit",
    "fetch-models-from-api": "dotenvx run -- tsx scripts/fetch-models-from-api.ts",
    "updata-provider-type": "tsx scripts/ProviderTypeUpdater.ts",
    "generate": "prisma generate && tsx scripts/fix-circular-imports.ts"
  },
  "exports": {
    "./prismaType": {
      "import": "./dist/prismaType.js",
      "types": "./dist/prismaType.d.ts"
    },
    "./schemas": {
      "types": "./dist/schemas.d.ts"
    },
    "./apiClient": {
      "import": "./dist/apiClient.js",
      "types": "./dist/apiClient.d.ts"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "type": "module",
  "engines": {
    "node": ">=22.13.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.63",
    "@dotenvx/dotenvx": "^1.44.1",
    "@hono/standard-validator": "^0.2.2",
    "@mastra/ai-sdk": "^1.0.3",
    "@mastra/core": "^1.2.0",
    "@mastra/libsql": "^1.2.0",
    "@mastra/loggers": "^1.0.1",
    "@mastra/memory": "^1.1.0",
    "@mastra/server": "^1.2.0",
    "@standard-community/standard-json": "^0.3.5",
    "@standard-community/standard-openapi": "^0.2.9",
    "ai": "^5.0.87",
    "hono": "^4.11.8",
    "hono-openapi": "^1.2.0",
    "mastra": "^1.2.0",
    "prisma-zod-generator": "^2.1.2",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@mocean/eslint-config": "workspace:*",
    "@mocean/typescript-config": "workspace:*",
    "@prisma/adapter-better-sqlite3": "^7.0.0",
    "@prisma/client": "^7.3.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^20.17.6",
    "@vitest/coverage-v8": "^2.1.0",
    "better-sqlite3": "^12.4.1",
    "eslint": "^9.15.0",
    "install": "^0.13.0",
    "playwright": "^1.41.0",
    "prettier": "^3.6.2",
    "prisma": "^7.3.0",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^2.1.0"
  }
}
